/**
 * 
 */
package gui.elements;

import javafx.stage.Stage;

/**
 * @author rasgarbayli
 *
 */
class GUIElementNullException extends Exception {

	/**
	 * 
	 */
	public GUIElementNullException() {
		// TODO Auto-generated constructor stub
	}

	/**
	 * @param message
	 */
	public GUIElementNullException(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	/**
	 * @param cause
	 */
	public GUIElementNullException(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}

	/**
	 * @param message
	 * @param cause
	 */
	public GUIElementNullException(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	/**
	 * @param message
	 * @param cause
	 * @param enableSuppression
	 * @param writableStackTrace
	 */
	public GUIElementNullException(String message, Throwable cause,
			boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
		// TODO Auto-generated constructor stub
	}
	
	private void generateMessageWindow(String message, Stage owner) {
		WarningWindow errorDialog = new WarningWindow(message, owner);
		errorDialog.show();
	}

}
